{
    "info": {
        "tableName": "platform-category",
        "singularName": "platform-category",
        "pluralName": "platform-categories",
        "displayName": "platformCategories",
        "description": "platformCategory ",
        "kind": "collectionType"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {
        "content-manager": {
            "visible": true
        },
        "content-type-builder": {
            "visible": false
        }
    },
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "link": {
            "type": "string",
            "required": true
        },
        "numberOfProducts": {
            "type": "integer",
            "required": true,
            "default": 0,
            "min": 0
        },
        "isChecked": {
            "type": "boolean",
            "default": false,
            "required": true
        },
        "marketPlaceFee": {
            "type": "decimal",
            "required": true,
            "default": 0,
            "min": 0
        },
        "cpsFee": {
            "type": "decimal",
            "required": true,
            "default": 0,
            "min": 0
        },
        "platform": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::platforms-scraper.platform",
            "inversedBy": "categories"
        }
    }
}