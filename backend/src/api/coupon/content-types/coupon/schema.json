{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "Coupon",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Standard_discount",
        "Percentage_discount"
      ]
    },
    "coupon_price": {
      "type": "decimal"
    },
    "free_shipping": {
      "type": "boolean",
      "default": false
    },
    "expired_at": {
      "type": "datetime"
    },
    "min_order": {
      "type": "decimal",
      "default": 0
    },
    "max_order": {
      "type": "decimal"
    },
    "included_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "incl_prod_coupon"
    },
    "excluded_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "excl_prod_coupon"
    },
    "includes_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "incl_cat_coupon"
    },
    "excluded_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "excl_cat_coupon"
    },
    "limit": {
      "type": "integer"
    },
    "limit_per_user": {
      "type": "integer"
    }
  }
}
