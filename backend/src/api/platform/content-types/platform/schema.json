{
  "kind": "collectionType",
  "collectionName": "platforms",
  "info": {
    "singularName": "platform",
    "pluralName": "platforms",
    "displayName": "Platform",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "entryURL": {
      "type": "string",
      "required": true
    },
    "merchantFeeCatalogue": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "platformCategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::platforms-scraper.platform-category",
      "mappedBy": "platform"
    },
    "export_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "export_to_platform"
    },
    "order_time": {
      "type": "time",
      "default": "11:50"
    }
  }
}
