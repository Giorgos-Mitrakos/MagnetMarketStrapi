{
  "kind": "collectionType",
  "info": {
    "tableName": "importxml",
    "singularName": "importxml",
    "pluralName": "importxmls",
    "displayName": "import",
    "description": "Import products",
    "kind": "collectionType"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "isUpload": {
      "type": "boolean",
      "default": false
    },
    "importedFile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "importedURL": {
      "type": "text"
    },
    "xPath": {
      "type": "text"
    },
    "minimumPrice": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "maximumPrice": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "lastRun": {
      "type": "datetime"
    },
    "report": {
      "type": "string"
    },
    "related_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "related_import"
    },
    "categories_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.categorymap",
      "mappedBy": "related_import"
    },
    "char_name_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.charnamemap",
      "mappedBy": "related_import"
    },
    "char_value_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.charvaluemap",
      "mappedBy": "related_import"
    },
    "stock_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.stockmap",
      "mappedBy": "related_import"
    },
    "isWhitelistSelected": {
      "type": "boolean",
      "default": true
    },
    "whitelist_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.whitelistmap",
      "mappedBy": "related_import"
    },
    "blacklist_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.blacklistmap",
      "mappedBy": "related_import"
    }
  }
}
